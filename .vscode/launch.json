    "version": "0.2.0",
    "configurations": [



    {
        "name": "profile show token",
        "type": "go",
        "request": "launch",
        "mode": "auto",
        "program": "${workspaceFolder}", 
        "args": ["profile", "show", "ego.logon.token"]
    },
        {
            "name": "Run interactively",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${workspaceFolder}", 
            "args": []
        },
        {
            "name": "ego test <all>",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${workspaceFolder}", 
            "args": ["test"]
        },
        {
            "name": "Run test.ego",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${workspaceFolder}", 
            "args": ["-d","compiler,symbols,trace", "run", "test.ego"]
        },
        {
            "name": "Debug test.ego",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${workspaceFolder}", 
            "args": ["run", "test.ego", "-d"]
        },
        {
            "name": "Trace test.ego",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${workspaceFolder}", 
            "args": ["run", "test.ego", "--trace"]
        },
        {
            "name": "test test.ego",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${workspaceFolder}", 
            "args": ["test", "test.ego"]
        },
        {
            "name": "Trace test test.ego",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${workspaceFolder}", 
            "args": ["-d", "trace", "test", "test.ego"]
        },
        {
            "name": "Logon to local server",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${workspaceFolder}", 
            "args": [ "logon", "-l", "localhost", "-u", "admin", "-p", "password"]
        },
        {
            "name": "Local ego server",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${workspaceFolder}", 
            "args": ["-d", "trace", "server", "run", "-k"]
        },
        {
            "name": "Detached ego server",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${workspaceFolder}", 
            "args": ["server", "start", "-k"]
        },
        {
            "name": "Local ego server/json",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${workspaceFolder}", 
            "args": ["server", "run", "-k", "-u", "foo.json"]
        },
        {
            "name": "Upsert user",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${workspaceFolder}", 
            "args": ["server", "users", "set", "-u", "tom", "-p", "fun", "--permissions", "logon,files"]
        },
        {
            "name": "List users",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${workspaceFolder}", 
            "args": ["-f", "json", "server", "users", "list"]
        },
        {
            "name": "Display Server cache",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${workspaceFolder}", 
            "args": ["--output-format", "json", "server", "cache", "list"]
        },
        {
            "name": "Display Remote server status",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${workspaceFolder}", 
            "args": ["server", "status", "http://localhost:8080"]
        }
    ]
}