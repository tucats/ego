    "version": "0.2.0",
    "configurations": [
        {
            "name": "run child task",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}", 
            "debugAdapter": "dlv-dap",
            "args": [ "--service", "tools/factor-request.json"]
        },  
        {
            "name": "run project",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}", 
            "debugAdapter": "dlv-dap",
            "args": [ "run", "--project", "."]
        },  
        {
            "name": "run with options but no verb",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}", 
            "debugAdapter": "dlv-dap",
            "args": ["--log", "cli"]
        },  
        {
            "name": "run with filename but no verb",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}", 
            "debugAdapter": "dlv-dap",
            "args": ["test.ego"]
        },    
        {
            "name": "start server",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}", 
            "debugAdapter": "dlv-dap",
            "args": ["server", "start", "-k"]
        },    
    {
        "name": "delete non-existant table",
        "type": "go",
        "request": "launch",
        "mode": "auto",
        "program": "${workspaceFolder}", 
        "debugAdapter": "dlv-dap",
        "args": ["--log", "rest", "db", "drop", "fizbo"]
    },
    {
        "name": "insert row into 'table'",
        "type": "go",
        "request": "launch",
        "mode": "auto",
        "program": "${workspaceFolder}", 
        "debugAdapter": "dlv-dap",
        "args": ["--log", "rest", "db", "insert", "table", "id=101", "name='Bob'"]
    },
    {
        "name": "execute SQL using dsn",
        "type": "go",
        "request": "launch",
        "mode": "auto",
        "program": "${workspaceFolder}", 
        "debugAdapter": "dlv-dap",
        "args": ["--log", "rest", "db", "sql", "--dsn", "test01", "select 42"]
    },
    {
        "name": "Set homebase profile bar=foo",
        "type": "go",
        "request": "launch",
        "mode": "auto",
        "program": "${workspaceFolder}", 
        "debugAdapter": "dlv-dap",
        "args": ["-p", "homebase", "config", "set", "bar=foo"]
    },
    {
        "name": "profile show token",
        "type": "go",
        "request": "launch",
        "mode": "auto",
        "program": "${workspaceFolder}", 
        "args": ["profile", "show", "ego.logon.token"]
    },
        {
            "name": "Run interactively",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${workspaceFolder}", 
            "args": ["--log", "app,token"]
        },
        {
            "name": "ego test <all>",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${workspaceFolder}", 
            "args": ["test"]
        },
        {
            "name": "ego test <admin.ego>",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${workspaceFolder}", 
            "args": ["test", "tests/server/admin.ego", "-t"]
        },        {
            "name": "Run test.ego",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${workspaceFolder}", 
            "args": [  "run", "test.ego", "--types=strict", "-t"]
        },
        {
            "name": "Debug test.ego",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${workspaceFolder}", 
            "args": ["run", "test.ego", "--log"]
        },
        {
            "name": "Debug cli parsing with parms",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${workspaceFolder}", 
            "args": ["-l", "cli", "config", "ego.compiler.extensions"]
        },
        {
            "name": "test with optimizer logging",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}", 
            "debugAdapter": "dlv-dap",
            "args": ["--log", "optimizer", "test", "-o=1"]
        },  
        {
            "name": "Trace test.ego",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${workspaceFolder}", 
            "args": ["run", "test.ego", "--trace"]
        },
        {
            "name": "test test.ego",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${workspaceFolder}", 
            "args": ["--log", "trace", "test", "test.ego", "--types=strict"]
        },
        {
            "name": "Trace test test.ego",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${workspaceFolder}", 
            "args": ["--log", "trace", "test", "test.ego"]
        },
        {
            "name": "Logon to local server",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${workspaceFolder}", 
            "args": [ "--log", "rest", "logon",  "-u", "admin", "-p", "password"]
        },
        {
            "name": "Local ego server (strict type checking)",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${workspaceFolder}", 
            "args": ["--log", "trace,compiler,debug,symbols", "server", "run", "--typing=strict"]
        },
        {
            "name": "Local ego server (relaxed type checking)",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${workspaceFolder}", 
            "args": ["--log", "rest,trace,compiler,debug,symbols", "server", "run", "--typing=relaxed"]
        },
        {
            "name": "Local ego server (default type checking)",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${workspaceFolder}", 
            "args": ["--log", "rest,auth,debug,trace", "server", "run"]
        },
        {
            "name": "Local ego server (dual mode)",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${workspaceFolder}", 
            "args": ["--log", "rest,sql,auth,debug", "server", "run", "--port", "443"]
        },
        {
            "name": "Local ego server (alternate auth server)",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${workspaceFolder}", 
            "args": ["-k", "--log", "rest,auth,debug", "server", "run", "--auth-server", "https://homebase.local"]
        },
        {
            "name": "CLI Testing",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${workspaceFolder}", 
            "args": ["-l", "rest", "server"]
        },
        {
            "name": "Detached ego server",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${workspaceFolder}", 
            "args": ["server", "start", "-k"]
        },
        {
            "name": "Local ego server/json",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${workspaceFolder}", 
            "args": ["server", "run", "-k", "-u", "foo.json"]
        },
        {
            "name": "Upsert user",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${workspaceFolder}", 
            "args": ["server", "users", "set", "-u", "tom", "-p", "fun", "--permissions", "logon,files"]
        },
        {
            "name": "List users",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${workspaceFolder}", 
            "args": ["-f", "json", "server", "users", "list"]
        },
        {
            "name": "Display Server cache",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${workspaceFolder}", 
            "args": ["server", "cache", "list", "--order-by", "count"]
        },
        {
            "name": "Display Remote server status",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${workspaceFolder}", 
            "args": ["server", "status", "http://localhost"]
        }
    ]
}