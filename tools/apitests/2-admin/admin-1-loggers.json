{
    "description": "Check status of loggers on server",
    "request": {
        "method": "GET",
        "endpoint": "{{SCHEME}}://{{HOST}}/admin/loggers",
        "headers": {
            "Content-Type": ["application/json"],
            "Accept": ["*/*"],
            "Authorization": ["Bearer {{API_TOKEN}}"]
        }
    },
    "response": {
        "status": 200,
        "headers": {
            "Content-Type": ["log.status+json"]
        }
    },
    "tests": [
        {
            "name": "api version",
            "query": "server.api",
            "value": "1"
        },
        {
            "name": "server id",
            "query": "server.id",
            "value": "{{SERVER_ID}}"
        },
        {
            "name": "APP logger",
            "query": "loggers.APP",
            "op": "exists"
        },
        {
            "name": "ASSET logger",
            "query": "loggers.ASSET",
            "op": "exists"
        },
        {
