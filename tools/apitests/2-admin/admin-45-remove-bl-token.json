{
    "description": "List tokens on blacklist",
    "request": {
        "method": "DELETE",
        "endpoint": "/admin/tokens/{{API_TOKEN_BL_ID}}", 
        "headers": {
            "Content-Type": ["application/json"],
            "Accept": ["*/*"],
            "Authorization": ["Bearer {{API_TOKEN}}"]
        }
    },
    "response": {
        "status": 200
    },
    "tests": [
        {
            "name": "api version",
            "query": "server.api",
            "value": "1"
        },
        {
            "name": "server id",
            "query": "server.id",
            "value": "{{SERVER_ID}}"
        }
    ]
}