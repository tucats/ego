{
    "description": "Get list of server users",
    "request": {
        "method": "POST",
        "endpoint": "{{SCHEME}}://{{HOST}}/admin/users/",
        "headers": {
            "Accept": ["application/vnd.ego.user+json"],
            "Content-Type": ["application/vmd.ego.user+json"],
            "Authorization": ["Bearer {{API_TOKEN}}"]
        },
        "file": "{{ROOT}}/5-users/users-3-create-user.payload"
    },
    "response": {
        "status": 200,
        "headers": {
            "Content-Type": ["application/vnd.ego.user+json"]
        }
    },
    "tests": [
        {
            "name": "api version",
            "query": "server.api",
            "value": "1"
        },
        {
            "name": "server id",
            "query": "server.id",
            "value": "{{SERVER_ID}}"
        },
        {
            "name": "user name",
            "query": "name",
            "value": "u{{USERUUID}}"
        },
        {
            "name": "user permission",
            "query": "permissions.0",
            "value": "bar"
        }
    ]
}