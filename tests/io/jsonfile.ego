@test "io: read and write json files" {

	a := { name: "Tom", age: 51 }

	fn := "test-" + uuid.New().String() + ".json"

	e := json.WriteFile(fn, a)
	@assert e == nil

	b, e := json.ReadFile(fn)
	@assert e == nil
	@assert b.name == "Tom"
	@assert float64(b.age) == float64(51)

	os.Remove(fn)
}

@pass
