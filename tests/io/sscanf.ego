@test "io: Sscanf"
{
    x := 0
    n, e := fmt.Sscanf("name 35", "age %d", &x)
    @assert T.Equal(n, 0)
    @assert T.Equal(x, 0)

    n, e = fmt.Sscanf("age 35", "age %d", &x)
    @assert T.Equal(n, 1)
    @assert T.Equal(x, 35)

    t := false
    n, e = fmt.Sscanf("is true", "is %t", &t)
    @assert T.Equal(n, 1)
    @assert T.True(t)

    f := 0.0
    n, e = fmt.Sscanf("3.14", "%f", &f)
    @assert T.Equal(n, 1)
    @assert T.Equal(f, 3.14)

    n, e = fmt.Sscanf("5 at 2.50", "%d at %f", &x, &f)
    @assert T.Equal(n, 2)
    @assert T.Equal(f, 2.50)
    @assert T.Equal(x, 5)

    n, e := fmt.Sscanf("thirty", "%d", &x)
    @assert T.Equal(n, 0)
    @assert T.True(e != nil )
    @assert T.Equal(x, 5)


	tp := float64

	// Scan a type
	n, e = fmt.Sscanf("[]int{ }", "%T", &tp)
	@assert n == 1
	@assert e == nil
	@assert tp == []int

	var (
		s string
        i int
		b bool
	)

	data := "123456true5.5"

	// Scan with lengths to break up the string without using spaces
	fmt.Sscanf(data, "%5s %1d %t", &s, &i, &b)

	@assert s == "12345"
	@assert i == 6
	@assert b == true


}
