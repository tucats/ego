// Test program for accessing a local gremlin system.

url := "ws://localhost:8182/gremlin"
g := 0

try {
	g = gopen(url)
} catch {
	print "Unable to connect to Gremlin server"
	call util.exit(1)
}

call gquery(g, `g.addV("test1").property("name", "Tom").property("rate", 18.75)`)
call gquery(g, `g.addV("test1").property("name", "Mary").property("rate", 77.75)`)
