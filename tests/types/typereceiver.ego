
@test "types: type-based receiver function"

{
    type Employee struct {
        name string
        age  int
    }

    func (e *Employee) setname(s string) {
        e.name = s
    }

    emp := Employee{age:55}
    emp.setname("Tom")

    @assert T.Equal(emp.name, "Tom")
    @assert T.Equal(emp.age, 55) 
    @assert T.Equal(type(emp), "Employee struct")  
}

@pass


