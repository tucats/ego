@test "types: var and type testing"

{
    var a1,a2,a3 int
    @assert T.Equal(type(a1) , "int" )
    @assert T.Equal(type(a2) , "int" )
    @assert T.Equal(type(a3) , "int" )

    var i int 
    var f float 
    var s string 
    var b bool 

    type x struct {
        name string,
        age int,
    }
    var xx x
    
    @assert T.Equal(type(i)  , "int"   ) 
    @assert T.Equal(type(f)  , "float" ) 
    @assert T.Equal(type(s)  , "string") 
    @assert T.Equal(type(b)  , "bool"  ) 
    @assert T.Equal(type(x)  , "type"  ) 
    @assert T.Equal(type(xx) , "x"     ) 
}
@pass
