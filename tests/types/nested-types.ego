@test "types: nested user-defined types"

{
    type EmpInfo struct {
        Name []string
        ID   int
    }

    type Employee struct {
        Info    EmpInfo
        Manager bool
    }

    e := Employee{
        Info: EmpInfo{ 
            Name: []string{"Bob", "Smith"},
            ID:   101,
        },
    }
    
    @assert T.Equal(e.Info.Name[0], "Bob")
    @assert T.Equal(e.Manager, false)
}
@pass
