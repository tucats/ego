
@test "types: user types in complex types"

{
    type EmpInfo struct {
        Name []string
        ID   int
    }

    type Employee struct {
        Info    EmpInfo
        Manager bool
    }

    e := Employee{
        Info: { 
            Name: {"Bob", "Smith"},
            ID:   101,
        },
    }

    type Employees map[int]Employee

    employeeList := Employees{}
    employeeList[101] = e

    @assert employeeList[101].Info.Name[0] == "Bob"
   
}
@pass
