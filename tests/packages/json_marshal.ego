@test "packages: json marshal of nested types"
{
    a := {}
    a.memory = {size:55}

    b := json.Marshal(a)
    @assert string(b) == `{"memory":{"size":55}}`

    c := []string{"one", "two"}
    e := {test:c}
    d := json.Marshal(e)
    @assert string(d) == `{"test":["one","two"]}`

    m := map[int]int{ 5 :55, 4: 44}
    n := {test:m}
    nn := json.Marshal(n)
    @assert string(nn) == `{"test":{"4":44,"5":55}}`
}
@pass