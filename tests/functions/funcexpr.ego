
@test "functions: function definitions as expressions"

{
   func x1( f interface{}, v interface{} ) interface{} {
      return f(v)
   }

   func x2( v float) float {
      return v * 2
   }
   v1 := x1(x2, 55.)
   v2 := x1( func(x int) int { 
         return x*3 
      }, 10)

   @assert T.Equal(v1, 110.0, "Incorrect value for declared function")
   @assert T.Equal(v2, 30,  "Incorrect value for function expression")
}
@pass
