/*
 * test1.ego
 *
 * Simple test program for ego processor.
 */

@test "functions: simple function call"
   {
   func dbl(x interface{}) interface{} {
      if reflect.Type(x) == float64 {
         return x * 2.0
      }
      
      return x * 2
   }

   @assert reflect.Type(dbl(4.0)) == float64
   @assert dbl(55)        == 110
   @assert dbl(-2)        == -4
}