@test "datamodel: sort package"
{

    // Sort an array of integers
    i := []int{5,2,6,4,0}
    i2 := sort.Ints(i)
    @assert T.Equal(i, i2)
    @assert T.Equal(i, []int{0, 2,4,5,6})

    // Sort an array of wrong type
    try {
        sort.Strings(i)
        @fail "Should not sort"
    } catch (e) {
        // Caught error
        //fmt.Println(e)
    }

    // Sort an array of floats
    f := []float{5,2,6,4,0}
    f2 := sort.Floats(f)
    @assert T.Equal(f, f2)
    @assert T.Equal(f, []float{0, 2,4,5,6})

    // Sort an array of strings
    s := []string{"apple", "", "pear", "cherry"}
    s2 := sort.Strings(s)
    @assert T.Equal(s, s2)
    @assert T.Equal(s, []string{ "", "apple", "cherry", "pear"})
}
@pass
