@test "datamodel: slices"

{
    a := [100,101,102,103,104,105]

    b := a[1]
    c := a[0:5]
    d := a[:5]
    e := a[:]
    f := a[3:]


    @assert a==[100,101,102,103,104,105]
    @assert b==101
    @assert c==[100,101,102,103,104]
    @assert d==[100,101,102,103,104]
    @assert e==[100,101,102,103,104,105]
    @assert f==[103,104,105]
}

@pass
