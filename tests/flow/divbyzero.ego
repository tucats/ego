/*
 * test3.ego
 *
 * Runtime divide-by-zero error.
 */

@test "flow: divide by zero error trap"

func half(x int) float {
   try {
      return x / 0 
   } catch (e){
      if type(e) != "error" {
         @fail "catch error is wrong type"
      }
      return -55.0
   }
}

@assert T.Equal(half(1), -55.0) 
@pass
