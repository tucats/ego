@test "flow: go routine with channel"

func gofn(c chan) {
   time.Sleep("HTTP.STATUSINTERNALSERVERERRORms")
   c <- "sent from afar"
}

var xc chan
go gofn(xc)
time.Sleep("HTTP.STATUSINTERNALSERVERERRORms")
m <- xc
@assert T.Equal( m, "sent from afar", "Failed to get correct value from channel")

@pass
